<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn.alaminahmed.quickmaths">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.learn.alaminahmed.quickmaths.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_app_id" />

        <activity android:name="com.learn.alaminahmed.quickmaths.LoginActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.RegisterActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.PlayActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.ProfileActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.ContactActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.InstructionActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.AdminActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.PointValue" />
        <activity android:name="com.learn.alaminahmed.quickmaths.RewardsActivity" />
        <activity android:name="com.learn.alaminahmed.quickmaths.CongratulationActivity"></activity>
    </application>

</manifest>